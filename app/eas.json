{
  "cli": {
    "version": ">= 12.14.1",
    "appVersionSource": "remote",
    "requireCommit": true
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "buildConfiguration": "Debug",
        "newArchEnabled": true
      },
      "android": {
        "buildType": "apk",
        "newArchEnabled": true
      },
      "env": {
        "BUILD_TYPE": "development",
        "CLINICAL_VALIDATION": "true",
        "NEW_ARCHITECTURE": "true"
      }
    },
    "preview": {
      "extends": "production",
      "distribution": "internal",
      "env": {
        "BUILD_TYPE": "preview",
        "CLINICAL_VALIDATION": "true",
        "HEALTHCARE_COMPLIANCE_MONITORING": "true",
        "PERFORMANCE_MONITORING": "true",
        "NEW_ARCHITECTURE": "true",
        "CRISIS_MONITORING": "true",
        "EXPO_PUBLIC_API_URL": "http://localhost:3000",
        "EXPO_PUBLIC_SUPABASE_URL": "$SUPABASE_URL_STAGING"
      },
      "cache": {
        "disabled": false,
        "cacheDefaultPaths": true
      }
    },
    "production": {
      "ios": {
        "simulator": false,
        "buildConfiguration": "Release",
        "newArchEnabled": true,
        "resourceClass": "m1-medium",
        "config": {
          "usesNonExemptEncryption": false
        },
        "entitlements": {
          "com.apple.security.application-groups": [
            "group.com.being.app.widgets"
          ]
        }
      },
      "android": {
        "buildType": "aab",
        "newArchEnabled": true,
        "resourceClass": "medium"
      },
      "env": {
        "BUILD_TYPE": "production",
        "CLINICAL_VALIDATION": "true",
        "HEALTHCARE_COMPLIANCE_MONITORING": "true",
        "PERFORMANCE_MONITORING": "true",
        "NEW_ARCHITECTURE": "true",
        "CRISIS_MONITORING": "true",
        "PRODUCTION_MONITORING": "true",
        "HIPAA_COMPLIANCE": "true",
        "THERAPEUTIC_MONITORING": "true"
      },
      "cache": {
        "disabled": false,
        "cacheDefaultPaths": true,
        "customPaths": [
          "app/node_modules"
        ]
      }
    },
    "production-emergency": {
      "extends": "production",
      "distribution": "store",
      "env": {
        "BUILD_TYPE": "production",
        "EMERGENCY_DEPLOYMENT": "true",
        "CRISIS_OVERRIDE": "true",
        "HEALTHCARE_COMPLIANCE_MONITORING": "true",
        "PERFORMANCE_MONITORING": "true",
        "NEW_ARCHITECTURE": "true",
        "PRODUCTION_MONITORING": "true",
        "HIPAA_COMPLIANCE": "true"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appId": "com.being.app",
        "ascAppId": "PLACEHOLDER_APP_ID",
        "appleTeamId": "PLACEHOLDER_TEAM_ID"
      },
      "android": {
        "serviceAccountKeyPath": "./android-service-account.json",
        "track": "production",
        "releaseStatus": "completed"
      }
    },
    "preview": {
      "ios": {
        "appId": "com.being.app",
        "ascAppId": "PLACEHOLDER_APP_ID",
        "appleTeamId": "PLACEHOLDER_TEAM_ID"
      },
      "android": {
        "serviceAccountKeyPath": "./android-service-account.json",
        "track": "internal",
        "releaseStatus": "draft"
      }
    }
  },
  "updates": {
    "url": "https://u.expo.dev/75ab182c-82f1-4f22-bd9c-d57358950bc6"
  },
  "experimental": {
    "prebuildCommand": "node scripts/prebuild-health-check.js"
  }
}