{
  "cli": {
    "version": ">= 5.4.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_ENV": "development",
        "EXPO_PUBLIC_CRISIS_HOTLINE": "988",
        "EXPO_PUBLIC_API_URL": "http://localhost:3000",
        "EXPO_PUBLIC_SENTRY_DSN": "",
        "EXPO_PUBLIC_ANALYTICS_ENABLED": "false",
        "EXPO_PUBLIC_CRASH_REPORTING": "false",
        "EXPO_PUBLIC_FEATURE_FLAGS": "dev_tools:true,debug_mode:true,beta_features:true"
      },
      "ios": {
        "buildConfiguration": "Debug",
        "bundleIdentifier": "com.fullmind.mbct.dev"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_ENV": "preview",
        "EXPO_PUBLIC_CRISIS_HOTLINE": "988",
        "EXPO_PUBLIC_API_URL": "https://api-staging.fullmind.app",
        "EXPO_PUBLIC_SENTRY_DSN": "$SENTRY_DSN_STAGING",
        "EXPO_PUBLIC_ANALYTICS_ENABLED": "true",
        "EXPO_PUBLIC_CRASH_REPORTING": "true",
        "EXPO_PUBLIC_FEATURE_FLAGS": "beta_features:true,performance_monitoring:true",
        "EXPO_PUBLIC_PRIVACY_POLICY_URL": "https://staging.fullmind.app/privacy",
        "EXPO_PUBLIC_TERMS_URL": "https://staging.fullmind.app/terms",
        "EXPO_PUBLIC_SUPPORT_URL": "https://staging.fullmind.app/support"
      },
      "ios": {
        "buildConfiguration": "Release",
        "bundleIdentifier": "com.fullmind.mbct.preview",
        "simulator": false
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_ENV": "production",
        "EXPO_PUBLIC_CRISIS_HOTLINE": "988",
        "EXPO_PUBLIC_API_URL": "https://api.fullmind.app",
        "EXPO_PUBLIC_SENTRY_DSN": "$SENTRY_DSN_PRODUCTION",
        "EXPO_PUBLIC_ANALYTICS_ENABLED": "true",
        "EXPO_PUBLIC_CRASH_REPORTING": "true",
        "EXPO_PUBLIC_PERFORMANCE_MONITORING": "true",
        "EXPO_PUBLIC_PRIVACY_POLICY_URL": "https://fullmind.app/privacy",
        "EXPO_PUBLIC_TERMS_URL": "https://fullmind.app/terms",
        "EXPO_PUBLIC_SUPPORT_URL": "https://fullmind.app/support",
        "EXPO_PUBLIC_CRISIS_RESOURCES_URL": "https://fullmind.app/crisis-resources",
        "EXPO_PUBLIC_FEATURE_FLAGS": "production_mode:true,performance_monitoring:true,crisis_detection:true",
        "EXPO_PUBLIC_ENCRYPTION_ENABLED": "true",
        "EXPO_PUBLIC_BIOMETRIC_AUTH": "true",
        "EXPO_PUBLIC_CLINICAL_ACCURACY_MODE": "true"
      },
      "ios": {
        "buildConfiguration": "Release",
        "bundleIdentifier": "com.fullmind.mbct",
        "simulator": false,
        "enterprise": false,
        "resourceClass": "m-medium",
        "config": {
          "usesNonExemptEncryption": false
        },
        "infoPlist": {
          "NSHealthShareUsageDescription": "FullMind integrates with Apple Health to provide comprehensive wellness tracking for your mental health journey.",
          "NSHealthUpdateUsageDescription": "FullMind can save your mindfulness session data to Apple Health to track your wellness progress.",
          "ITSAppUsesNonExemptEncryption": false,
          "LSApplicationCategoryType": "public.app-category.healthcare-fitness",
          "NSUserTrackingUsageDescription": "This helps us improve our therapeutic content and provide personalized mindfulness experiences while keeping your data private.",
          "CFBundleURLTypes": [
            {
              "CFBundleURLName": "fullmind-deep-linking",
              "CFBundleURLSchemes": ["fullmind"]
            }
          ],
          "UIBackgroundModes": ["background-processing", "background-fetch"],
          "BGTaskSchedulerPermittedIdentifiers": ["com.fullmind.mbct.reminder", "com.fullmind.mbct.crisis-check"]
        },
        "entitlements": {
          "com.apple.security.application-groups": ["group.com.fullmind.mbct.widgets"],
          "com.apple.developer.healthkit": true,
          "com.apple.developer.healthkit.access": ["health-records"]
        }
      },
      "android": {
        "buildType": "aab",
        "gradleCommand": ":app:bundleRelease",
        "resourceClass": "medium",
        "config": {
          "minSdkVersion": 23,
          "compileSdkVersion": 34,
          "targetSdkVersion": 34,
          "buildToolsVersion": "34.0.0"
        },
        "permissions": [
          "android.permission.RECORD_AUDIO",
          "android.permission.MODIFY_AUDIO_SETTINGS",
          "android.permission.WAKE_LOCK",
          "android.permission.RECEIVE_BOOT_COMPLETED",
          "android.permission.SCHEDULE_EXACT_ALARM",
          "android.permission.USE_BIOMETRIC",
          "android.permission.USE_FINGERPRINT",
          "android.permission.INTERNET",
          "android.permission.ACCESS_NETWORK_STATE"
        ],
        "intentFilters": [
          {
            "action": "android.intent.action.VIEW",
            "category": ["android.intent.category.DEFAULT", "android.intent.category.BROWSABLE"],
            "data": {
              "scheme": "fullmind"
            }
          }
        ]
      }
    },
    "production-ios": {
      "extends": "production",
      "ios": {
        "buildConfiguration": "Release",
        "bundleIdentifier": "com.fullmind.mbct",
        "simulator": false,
        "enterprise": false,
        "resourceClass": "m-medium",
        "autoIncrement": "buildNumber"
      }
    },
    "production-android": {
      "extends": "production",
      "android": {
        "buildType": "aab",
        "gradleCommand": ":app:bundleRelease",
        "resourceClass": "medium",
        "autoIncrement": "versionCode"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "$APPLE_ID",
        "ascAppId": "$ASC_APP_ID",
        "appleTeamId": "$APPLE_TEAM_ID",
        "sku": "fullmind-mbct-ios",
        "language": "en-US",
        "companyName": "FullMind Health Technologies",
        "appName": "FullMind MBCT",
        "copyright": "Â© 2024 FullMind Health Technologies. All rights reserved.",
        "primaryCategory": "MEDICAL",
        "secondaryCategory": "HEALTH_AND_FITNESS",
        "primarySubcategoryOne": "HEALTH_AND_FITNESS",
        "contentRights": {
          "containsThirdPartyContent": false,
          "hasRights": true
        },
        "usesIdfa": false,
        "isFirstPartyAd": false,
        "servesAds": false,
        "contentRating": "4+",
        "priceTier": "0",
        "releaseType": "MANUAL",
        "earliestReleaseDate": null,
        "phaseType": "IMMEDIATE"
      },
      "android": {
        "serviceAccountKeyPath": "$GOOGLE_SERVICE_ACCOUNT_KEY_PATH",
        "track": "production",
        "releaseStatus": "completed",
        "inAppUpdatePriority": 0,
        "rollout": 1.0,
        "changesNotSentForReview": false
      }
    },
    "preview": {
      "ios": {
        "appleId": "$APPLE_ID",
        "ascAppId": "$ASC_APP_ID",
        "appleTeamId": "$APPLE_TEAM_ID",
        "sku": "fullmind-mbct-ios-preview"
      },
      "android": {
        "serviceAccountKeyPath": "$GOOGLE_SERVICE_ACCOUNT_KEY_PATH",
        "track": "internal",
        "releaseStatus": "draft"
      }
    }
  }
}