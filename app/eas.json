{
  "cli": {
    "version": ">= 5.4.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_ENV": "development",
        "EXPO_PUBLIC_CRISIS_HOTLINE": "988",
        "EXPO_PUBLIC_API_URL": "http://localhost:3000",
        "EXPO_PUBLIC_SENTRY_DSN": "https://fake-dsn@sentry.io/development",
        "EXPO_PUBLIC_ANALYTICS_ENABLED": "false",
        "EXPO_PUBLIC_CRASH_REPORTING": "false",
        "EXPO_PUBLIC_FEATURE_FLAGS": "dev_tools:true,debug_mode:true,beta_features:true,cloud_sync:false",
        "EXPO_PUBLIC_SUPABASE_URL": "$SUPABASE_URL_DEV",
        "EXPO_PUBLIC_SUPABASE_ANON_KEY": "$SUPABASE_ANON_KEY_DEV",
        "EXPO_PUBLIC_CLOUD_FEATURES_ENABLED": "false"
      },
      "ios": {
        "buildConfiguration": "Debug"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_ENV": "preview",
        "EXPO_PUBLIC_CRISIS_HOTLINE": "988",
        "EXPO_PUBLIC_API_URL": "https://api-staging.being.app",
        "EXPO_PUBLIC_SENTRY_DSN": "$SENTRY_DSN_STAGING",
        "EXPO_PUBLIC_ANALYTICS_ENABLED": "true",
        "EXPO_PUBLIC_CRASH_REPORTING": "true",
        "EXPO_PUBLIC_FEATURE_FLAGS": "beta_features:true,performance_monitoring:true,cloud_sync:false",
        "EXPO_PUBLIC_PRIVACY_POLICY_URL": "https://staging.being.app/privacy",
        "EXPO_PUBLIC_TERMS_URL": "https://staging.being.app/terms",
        "EXPO_PUBLIC_SUPPORT_URL": "https://staging.being.app/support",
        "EXPO_PUBLIC_SUPABASE_URL": "$SUPABASE_URL_STAGING",
        "EXPO_PUBLIC_SUPABASE_ANON_KEY": "$SUPABASE_ANON_KEY_STAGING",
        "EXPO_PUBLIC_CLOUD_FEATURES_ENABLED": "false"
      },
      "ios": {
        "buildConfiguration": "Release",
        "simulator": false
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_ENV": "production",
        "EXPO_PUBLIC_CRISIS_HOTLINE": "988",
        "EXPO_PUBLIC_API_URL": "https://api.being.app",
        "EXPO_PUBLIC_SENTRY_DSN": "$SENTRY_DSN_PRODUCTION",
        "EXPO_PUBLIC_ANALYTICS_ENABLED": "true",
        "EXPO_PUBLIC_CRASH_REPORTING": "true",
        "EXPO_PUBLIC_PERFORMANCE_MONITORING": "true",
        "EXPO_PUBLIC_PRIVACY_POLICY_URL": "https://being.app/privacy",
        "EXPO_PUBLIC_TERMS_URL": "https://being.app/terms",
        "EXPO_PUBLIC_SUPPORT_URL": "https://being.app/support",
        "EXPO_PUBLIC_CRISIS_RESOURCES_URL": "https://being.app/crisis-resources",
        "EXPO_PUBLIC_FEATURE_FLAGS": "production_mode:true,performance_monitoring:true,crisis_detection:true,cloud_sync:false",
        "EXPO_PUBLIC_ENCRYPTION_ENABLED": "true",
        "EXPO_PUBLIC_BIOMETRIC_AUTH": "true",
        "EXPO_PUBLIC_CLINICAL_ACCURACY_MODE": "true",
        "EXPO_PUBLIC_SUPABASE_URL": "$SUPABASE_URL_PRODUCTION",
        "EXPO_PUBLIC_SUPABASE_ANON_KEY": "$SUPABASE_ANON_KEY_PRODUCTION",
        "EXPO_PUBLIC_CLOUD_FEATURES_ENABLED": "false"
      }
    }
  }
}