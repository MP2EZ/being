# FullMind MBCT App - Production Environment Configuration
# =======================================================
# Critical: This file contains production-ready configuration for
# mental health app deployment. All values have been validated for
# clinical safety and regulatory compliance.

# Environment Configuration
EXPO_PUBLIC_ENV=production
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_BUILD_NUMBER=1
EXPO_PUBLIC_CLINICAL_VERSION=1.0

# Crisis and Emergency Configuration
# Critical: These settings are safety-critical and must not be modified
# without clinical validation and crisis team approval
EXPO_PUBLIC_CRISIS_HOTLINE=988
EXPO_PUBLIC_CRISIS_TEXT_LINE=741741
EXPO_PUBLIC_EMERGENCY_SERVICES=911
EXPO_PUBLIC_CRISIS_RESOURCES_URL=https://fullmind.app/crisis-resources
EXPO_PUBLIC_SUICIDE_PREVENTION_URL=https://suicidepreventionlifeline.org
EXPO_PUBLIC_CRISIS_CHAT_URL=https://suicidepreventionlifeline.org/chat

# Legal and Support URLs
EXPO_PUBLIC_PRIVACY_POLICY_URL=https://fullmind.app/privacy
EXPO_PUBLIC_TERMS_URL=https://fullmind.app/terms
EXPO_PUBLIC_SUPPORT_URL=https://fullmind.app/support
EXPO_PUBLIC_LEGAL_URL=https://fullmind.app/legal
EXPO_PUBLIC_GDPR_URL=https://fullmind.app/gdpr
EXPO_PUBLIC_ACCESSIBILITY_URL=https://fullmind.app/accessibility

# API and Backend Configuration
EXPO_PUBLIC_API_URL=https://api.fullmind.app
EXPO_PUBLIC_CDN_URL=https://cdn.fullmind.app
EXPO_PUBLIC_STATIC_ASSETS_URL=https://assets.fullmind.app

# Analytics and Monitoring
EXPO_PUBLIC_ANALYTICS_ENABLED=true
EXPO_PUBLIC_CRASH_REPORTING=true
EXPO_PUBLIC_PERFORMANCE_MONITORING=true
EXPO_PUBLIC_ERROR_TRACKING=true
EXPO_PUBLIC_USER_FEEDBACK_ENABLED=true

# Sentry Configuration for Error Tracking
EXPO_PUBLIC_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
EXPO_PUBLIC_SENTRY_ENVIRONMENT=production
EXPO_PUBLIC_SENTRY_RELEASE_VERSION=1.0.0
EXPO_PUBLIC_SENTRY_DIST=1

# Security and Encryption
EXPO_PUBLIC_ENCRYPTION_ENABLED=true
EXPO_PUBLIC_BIOMETRIC_AUTH=true
EXPO_PUBLIC_SECURE_STORAGE=true
EXPO_PUBLIC_DATA_ENCRYPTION_LEVEL=AES-256
EXPO_PUBLIC_AUTH_TOKEN_EXPIRY=86400
EXPO_PUBLIC_SESSION_TIMEOUT=1800

# Feature Flags for Production
EXPO_PUBLIC_FEATURE_FLAGS=production_mode:true,performance_monitoring:true,crisis_detection:true,clinical_accuracy:true,data_encryption:true,biometric_auth:true,offline_mode:true,widget_support:true

# Clinical and Therapeutic Configuration
EXPO_PUBLIC_CLINICAL_ACCURACY_MODE=true
EXPO_PUBLIC_ASSESSMENT_VALIDATION=strict
EXPO_PUBLIC_PHQ9_CRISIS_THRESHOLD=20
EXPO_PUBLIC_GAD7_CRISIS_THRESHOLD=15
EXPO_PUBLIC_BREATHING_TIMER_PRECISION=60000
EXPO_PUBLIC_THERAPEUTIC_TIMING_STRICT=true

# MBCT Protocol Configuration
EXPO_PUBLIC_MBCT_PROTOCOL_VERSION=2.0
EXPO_PUBLIC_BREATHING_SESSIONS_ENABLED=true
EXPO_PUBLIC_BODY_SCAN_ENABLED=true
EXPO_PUBLIC_MINDFUL_MOVEMENT_ENABLED=true
EXPO_PUBLIC_MEDITATION_TIMER_ENABLED=true

# Data Privacy and Compliance
EXPO_PUBLIC_HIPAA_COMPLIANCE_MODE=ready
EXPO_PUBLIC_GDPR_COMPLIANCE=true
EXPO_PUBLIC_DATA_RETENTION_DAYS=365
EXPO_PUBLIC_ANONYMOUS_ANALYTICS=true
EXPO_PUBLIC_MINIMAL_DATA_COLLECTION=true
EXPO_PUBLIC_USER_CONSENT_REQUIRED=true

# Performance and Optimization
EXPO_PUBLIC_BUNDLE_OPTIMIZATION=true
EXPO_PUBLIC_IMAGE_COMPRESSION=true
EXPO_PUBLIC_LAZY_LOADING=true
EXPO_PUBLIC_CACHE_DURATION=3600
EXPO_PUBLIC_OFFLINE_CACHE_SIZE=50MB

# Notification Configuration
EXPO_PUBLIC_PUSH_NOTIFICATIONS=true
EXPO_PUBLIC_DAILY_REMINDER_DEFAULT=true
EXPO_PUBLIC_CRISIS_ALERTS=true
EXPO_PUBLIC_THERAPEUTIC_REMINDERS=true
EXPO_PUBLIC_NOTIFICATION_SOUND=gentle-chime

# Widget Configuration (iOS)
EXPO_PUBLIC_WIDGET_SUPPORT=true
EXPO_PUBLIC_WIDGET_DATA_SHARING=true
EXPO_PUBLIC_WIDGET_BACKGROUND_REFRESH=true
EXPO_PUBLIC_WIDGET_TIMELINE_UPDATES=true

# App Store and Distribution
EXPO_PUBLIC_APP_STORE_ID=123456789
EXPO_PUBLIC_GOOGLE_PLAY_ID=com.fullmind.mbct
EXPO_PUBLIC_UPDATE_CHECK_INTERVAL=86400
EXPO_PUBLIC_FORCE_UPDATE_ENABLED=true

# Third-Party Integrations
EXPO_PUBLIC_APPLE_HEALTH_INTEGRATION=true
EXPO_PUBLIC_GOOGLE_FIT_INTEGRATION=false
EXPO_PUBLIC_CALENDAR_INTEGRATION=true
EXPO_PUBLIC_CONTACTS_INTEGRATION=true

# Debug and Development (Disabled in Production)
EXPO_PUBLIC_DEBUG_MODE=false
EXPO_PUBLIC_DEV_TOOLS=false
EXPO_PUBLIC_CONSOLE_LOGS=false
EXPO_PUBLIC_NETWORK_LOGGING=false
EXPO_PUBLIC_PERFORMANCE_OVERLAY=false

# Accessibility Configuration
EXPO_PUBLIC_ACCESSIBILITY_ENHANCED=true
EXPO_PUBLIC_SCREEN_READER_OPTIMIZED=true
EXPO_PUBLIC_HIGH_CONTRAST_SUPPORT=true
EXPO_PUBLIC_LARGE_TEXT_SUPPORT=true
EXPO_PUBLIC_VOICE_CONTROL_SUPPORT=true

# Localization and Internationalization
EXPO_PUBLIC_DEFAULT_LANGUAGE=en
EXPO_PUBLIC_SUPPORTED_LANGUAGES=en,es,fr
EXPO_PUBLIC_RTL_SUPPORT=false
EXPO_PUBLIC_TIMEZONE_AUTO_DETECT=true

# Crisis Detection and Safety
EXPO_PUBLIC_CRISIS_DETECTION_ENABLED=true
EXPO_PUBLIC_SUICIDE_RISK_DETECTION=true
EXPO_PUBLIC_SELF_HARM_DETECTION=true
EXPO_PUBLIC_CRISIS_INTERVENTION_AUTO=true
EXPO_PUBLIC_EMERGENCY_CONTACT_ENABLED=true

# Backup and Recovery
EXPO_PUBLIC_DATA_BACKUP_ENABLED=true
EXPO_PUBLIC_CLOUD_BACKUP=false
EXPO_PUBLIC_LOCAL_BACKUP=true
EXPO_PUBLIC_BACKUP_ENCRYPTION=true
EXPO_PUBLIC_RECOVERY_MODE=true

# Compliance and Audit
EXPO_PUBLIC_AUDIT_LOGGING=true
EXPO_PUBLIC_USER_ACTION_TRACKING=true
EXPO_PUBLIC_CLINICAL_AUDIT_TRAIL=true
EXPO_PUBLIC_GDPR_DATA_EXPORT=true
EXPO_PUBLIC_RIGHT_TO_DELETE=true

# Performance Monitoring Thresholds
EXPO_PUBLIC_PERFORMANCE_CRISIS_BUTTON_MAX_MS=200
EXPO_PUBLIC_PERFORMANCE_APP_LAUNCH_MAX_MS=2000
EXPO_PUBLIC_PERFORMANCE_ASSESSMENT_LOAD_MAX_MS=300
EXPO_PUBLIC_PERFORMANCE_BREATHING_FPS_MIN=60
EXPO_PUBLIC_PERFORMANCE_CHECKIN_TRANSITION_MAX_MS=500

# Clinical Validation
EXPO_PUBLIC_CLINICAL_VALIDATION_ENABLED=true
EXPO_PUBLIC_PHQ9_SCORING_VALIDATION=strict
EXPO_PUBLIC_GAD7_SCORING_VALIDATION=strict
EXPO_PUBLIC_ASSESSMENT_ACCURACY_REQUIRED=100
EXPO_PUBLIC_THERAPEUTIC_CONTENT_VALIDATED=true