EXPO_PUBLIC_FEATURE_FLAGS=production_mode:true,performance_monitoring:true,crisis_detection:true,clinical_accuracy:true,data_encryption:true,biometric_auth:true,offline_mode:true,widget_support:true,cloud_sync:false,emergency_sync:false,cross_device_sync:false

