{
  "expo": {
    "name": "Being.",
    "slug": "being-mbct",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "description": "Evidence-based mindfulness and cognitive therapy practices for mental wellness. Features daily check-ins, PHQ-9/GAD-7 assessments, and crisis support resources.",
    "keywords": [
      "mindfulness",
      "mental health",
      "MBCT",
      "wellness",
      "meditation",
      "anxiety",
      "depression"
    ],
    "primaryColor": "#4A7C59",
    "privacy": "unlisted",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.being.mbct",
      "buildNumber": "1",
      "icon": "./assets/icon.png",
      "infoPlist": {
        "NSHealthShareUsageDescription": "Being. integrates with Apple Health to provide comprehensive wellness tracking for your mental health journey.",
        "NSHealthUpdateUsageDescription": "Being. can save your mindfulness session data to Apple Health to track your wellness progress.",
        "ITSAppUsesNonExemptEncryption": false,
        "LSApplicationCategoryType": "public.app-category.healthcare-fitness",
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "being-deep-linking",
            "CFBundleURLSchemes": [
              "being"
            ]
          }
        ]
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "entitlements": {
        "com.apple.security.application-groups": [
          "group.com.being.mbct.widgets"
        ]
      }
    },
    "android": {
      "package": "com.being.mbct",
      "versionCode": 1,
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.WAKE_LOCK",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ],
      "intentFilters": [
        {
          "action": "android.intent.action.VIEW",
          "category": [
            "android.intent.category.DEFAULT",
            "android.intent.category.BROWSABLE"
          ],
          "data": {
            "scheme": "being"
          }
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "name": "Being. MBCT",
      "shortName": "Being.",
      "lang": "en",
      "scope": "/",
      "themeColor": "#4A7C59",
      "description": "Mindfulness-Based Cognitive Therapy for mental wellness"
    },
    "plugins": [
      "expo-secure-store",
      "expo-local-authentication",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#4A7C59",
          "defaultChannel": "default"
        }
      ],
      "./plugins/expo-being-widgets",
      "expo-web-browser"
    ],
    "extra": {
      "clinicalVersion": "1.0",
      "privacyPolicyUrl": "https://being.app/privacy",
      "termsOfServiceUrl": "https://being.app/terms",
      "supportUrl": "https://being.app/support",
      "crisisHotline": "988",
      "buildType": "production",
      "eas": {
        "projectId": "75ab182c-82f1-4f22-bd9c-d57358950bc6"
      }
    },
    "owner": "palouselabs"
  }
}
