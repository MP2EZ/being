{
  "name": "app",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --ci --watchAll=false",
    "test:clinical": "jest --testPathPattern=clinical --verbose",
    "test:crisis-detection": "NODE_OPTIONS='--max-old-space-size=8192' jest src/services/crisis/__tests__/CrisisDetectionValidation.test.ts --verbose --testTimeout=60000",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration --verbose --testTimeout=30000",
    "test:performance": "jest --testPathPattern=performance --verbose",
    "test:accessibility": "jest --testPathPattern=accessibility --verbose",
    "test:security": "jest --testPathPattern=security --verbose",
    "test:encryption": "jest --testPathPattern=EncryptionService --verbose",
    "test:secure-storage": "jest --testPathPattern=SecureDataStore --verbose",
    "test:crisis-quick": "JEST_QUICK=true jest --testNamePattern=\"crisis|Crisis\" --ci --silent --testTimeout=5000 --maxWorkers=4",
    "test:clinical-quick": "JEST_QUICK=true jest --testNamePattern=\"clinical|phq|gad\"",
    "test:offline": "jest --testPathPattern=offline --verbose --testTimeout=60000",
    "test:offline-crisis": "jest --testNamePattern=\"Offline Crisis Management\" --verbose --testTimeout=30000",
    "validate:clinical": "npm run test:clinical && echo '‚úÖ Clinical accuracy tests passed'",
    "validate:accessibility": "npm run test:accessibility && echo '‚úÖ Accessibility tests passed'",
    "validate:performance": "npm run test:performance && echo '‚úÖ Performance tests passed'",
    "validate:clinical-complete": "npm run test:clinical && echo '‚úÖ Complete clinical accuracy validation passed'",
    "validate:crisis-authority": "npm run test:crisis-quick && npm run perf:crisis && echo '‚úÖ Crisis Authority validation passed'",
    "validate:compliance-authority": "npm run test:encryption && npm run test:secure-storage && echo '‚úÖ Compliance Authority validation passed'",
    "validate:clinical-authority": "npm run validate:clinical-complete && echo '‚úÖ Clinical Authority validation passed'",
    "validate:offline-crisis": "npm run test:offline-crisis && echo '‚úÖ Offline crisis management validation passed'",
    "perf:breathing": "jest --testNamePattern=\"BreathingCircle Performance\" --verbose --detectOpenHandles",
    "perf:crisis": "jest --testNamePattern=\"Crisis.*Performance\" --verbose --detectOpenHandles",
    "perf:launch": "jest --testNamePattern=\"App Launch Performance\" --verbose --detectOpenHandles",
    "perf:all": "npm run perf:breathing && npm run perf:crisis && npm run perf:launch",
    "perf:offline": "jest --testNamePattern=\"Offline.*Performance\" --verbose --detectOpenHandles",
    "emergency:crisis-safety-quick": "timeout 15s npm run test -- --testNamePattern=\"Crisis.*Quick|Emergency.*Quick|988.*Quick\" --ci --silent --testTimeout=5000",
    "automation:quick": "node scripts/local-test-automation.js quick",
    "automation:full": "node scripts/local-test-automation.js comprehensive",
    "automation:performance": "node scripts/local-test-automation.js performance",
    "automation:pre-commit": "node scripts/local-test-automation.js pre-commit",
    "automation:dev": "node scripts/local-test-automation.js development",
    "automation:watch": "node scripts/local-test-automation.js watch",
    "local:validate": "npm run automation:quick && echo '‚úÖ Local validation complete'",
    "local:crisis-check": "npm run test:crisis-quick && npm run perf:crisis && echo '‚úÖ Crisis safety validation complete'",
    "local:clinical-check": "npm run test:clinical && npm run validate:clinical-complete && echo '‚úÖ Clinical accuracy validation complete'",
    "hooks:setup": "node scripts/setup-git-hooks.js setup",
    "hooks:setup-minimal": "node scripts/setup-git-hooks.js setup-minimal",
    "hooks:setup-full": "node scripts/setup-git-hooks.js setup-full",
    "hooks:remove": "node scripts/setup-git-hooks.js remove",
    "hooks:status": "node scripts/setup-git-hooks.js status",
    "reports:dashboard": "node scripts/test-report-generator.js dashboard",
    "reports:performance": "node scripts/test-report-generator.js performance",
    "reports:coverage": "node scripts/test-report-generator.js coverage",
    "reports:all": "node scripts/test-report-generator.js all",
    "quick:validate": "node scripts/quick-validation.js smart",
    "quick:crisis": "node scripts/quick-validation.js crisis",
    "quick:clinical": "node scripts/quick-validation.js clinical",
    "quick:syntax": "node scripts/quick-validation.js syntax",
    "quick:perf": "node scripts/quick-validation.js performance",
    "quick:fix": "node scripts/quick-validation.js fix",
    "quick:watch": "node scripts/quick-validation.js watch",
    "quick:interactive": "node scripts/quick-validation.js interactive",
    "dev:test-smart": "node scripts/dev-test-runner.js smart",
    "dev:test-focused": "node scripts/dev-test-runner.js focused",
    "dev:test-debug": "node scripts/dev-test-runner.js debug",
    "dev:test-fast": "node scripts/dev-test-runner.js fast",
    "dev:test-coverage": "node scripts/dev-test-runner.js coverage",
    "platform:list": "node scripts/cross-platform-testing.js list",
    "platform:ios": "node scripts/cross-platform-testing.js ios",
    "platform:android": "node scripts/cross-platform-testing.js android",
    "platform:both": "node scripts/cross-platform-testing.js both",
    "platform:setup": "node scripts/cross-platform-testing.js setup",
    "platform:parallel": "node scripts/cross-platform-testing.js both --parallel",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:clinical": "eslint src --ext .ts,.tsx --config .eslintrc.clinical.js",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "npx tsc --noEmit --incremental",
    "precommit": "npm run typecheck && npm run lint:clinical && npm run test:clinical && npm run test:unit",
    "prepush": "npm run test:ci && npm run validate:clinical-complete",
    "version-check": "node -e \"const pkg=require('./package.json'); if(pkg.dependencies.react !== '19.1.0') throw new Error('React must be 19.1.0 for RN 0.81.4 compatibility')\"",
    "security:sast": "echo \"üîç Running SAST Analysis...\" && npm audit --audit-level moderate && echo \"‚úÖ SAST Analysis completed\"",
    "security:deps": "npm audit --audit-level high",
    "security:full": "npm run security:sast && npm run security:deps",
    "deploy:validate-rollback": "echo \"üîÑ Validating rollback safety...\" && node -e \"console.log('Rollback validation would run here in production')\""
  },
  "overrides": {
    "react": "19.1.0",
    "react-test-renderer": "19.1.0",
    "@types/react": "19.1.10"
  },
  "resolutions": {
    "react": "19.1.0",
    "react-test-renderer": "19.1.0",
    "@types/react": "19.1.10"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-community/slider": "5.0.1",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "@stripe/stripe-react-native": "0.50.3",
    "@supabase/supabase-js": "^2.39.3",
    "expo": "~54.0.9",
    "expo-apple-authentication": "~8.0.7",
    "expo-asset": "^12.0.9",
    "expo-auth-session": "~7.0.8",
    "expo-clipboard": "~8.0.7",
    "expo-crypto": "~15.0.7",
    "expo-dev-client": "~6.0.12",
    "expo-device": "~8.0.8",
    "expo-file-system": "^19.0.14",
    "expo-haptics": "~15.0.7",
    "expo-in-app-purchases": "^14.5.0",
    "expo-local-authentication": "~17.0.7",
    "expo-notifications": "~0.32.11",
    "expo-print": "~15.0.7",
    "expo-secure-store": "~15.0.7",
    "expo-sharing": "~14.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-web-browser": "~15.0.7",
    "immer": "^10.1.3",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-aes-crypto": "^3.2.1",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keychain": "^10.0.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-test-renderer": "19.1.0",
    "stripe": "^14.0.0",
    "uuid": "^13.0.0",
    "zod": "^3.22.4",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.8",
    "@types/node": "^24.3.1",
    "@types/react": "19.1.10",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "babel-preset-expo": "^54.0.2",
    "detox": "^20.13.5",
    "eslint": "^9.34.0",
    "eslint-config-universe": "^15.0.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.12",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
  },
  "private": true
}
