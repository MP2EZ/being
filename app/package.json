{
  "name": "app",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:clinical": "jest --testPathPattern=clinical --verbose",
    "test:integration": "jest --testPathPattern=integration --verbose --testTimeout=30000",
    "test:performance": "jest --testPathPattern=performance --verbose",
    "test:accessibility": "jest --testPathPattern=accessibility --verbose",
    "test:unit": "jest --testPathPattern=unit",
    "test:ci": "jest --coverage --ci --watchAll=false",
    "validate:clinical": "npm run test:clinical && echo '✅ Clinical accuracy tests passed'",
    "validate:accessibility": "npm run test:accessibility && echo '✅ Accessibility tests passed'",
    "validate:performance": "npm run test:performance && echo '✅ Performance tests passed'",
    "perf:breathing": "jest --testNamePattern=\"BreathingCircle Performance\" --verbose --detectOpenHandles",
    "perf:crisis": "jest --testNamePattern=\"Crisis.*Performance\" --verbose --detectOpenHandles",
    "perf:launch": "jest --testNamePattern=\"App Launch Performance\" --verbose --detectOpenHandles",
    "perf:all": "npm run perf:breathing && npm run perf:crisis && npm run perf:launch",
    "perf:monitor": "node -e \"console.log('Performance monitoring tools available in PerformanceMonitor.ts')\"",
    "validate:encryption": "ts-node scripts/validate-encryption.ts && echo '✅ Encryption validation passed'",
    "validate:types": "ts-node scripts/validate-types.ts && echo '✅ TypeScript clinical validation passed'",
    "validate:clinical-complete": "npm run validate:types && npm run test:clinical && echo '✅ Complete clinical accuracy validation passed'",
    "test:encryption": "jest --testPathPattern=EncryptionService --verbose",
    "test:secure-storage": "jest --testPathPattern=SecureDataStore --verbose",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:clinical": "eslint src --ext .ts,.tsx --config .eslintrc.clinical.js",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "npx tsc --noEmit",
    "typecheck:strict": "npx tsc --noEmit --strict --exactOptionalPropertyTypes --noUncheckedIndexedAccess",
    "precommit": "npm run typecheck:strict && npm run lint:clinical && npm run validate:types && npm run test:clinical && npm run test:unit",
    "prepush": "npm run test:ci && npm run validate:clinical-complete"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-community/slider": "4.5.6",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "expo": "~53.0.22",
    "expo-clipboard": "~7.1.5",
    "expo-crypto": "^14.1.5",
    "expo-haptics": "~14.1.4",
    "expo-notifications": "~0.31.4",
    "expo-print": "~14.1.4",
    "expo-secure-store": "^14.2.3",
    "expo-sharing": "~13.1.5",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-keychain": "^10.0.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "zod": "^3.22.4",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.8",
    "@types/node": "^24.3.1",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "detox": "^20.13.5",
    "eslint": "^9.34.0",
    "eslint-config-universe": "^15.0.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3"
  },
  "private": true
}
